${_MZ_CONAN_PROFILE_INCLUDES}
include(${_MZ_CONAN_DIR}/profile.unix.conan)

[settings]
os=Linux
os_build=Linux
arch=x86_64
arch_build=x86_64
compiler=clang
compiler.version=6.0
compiler.libcxx=libstdc++11
build_type=Release

[env]
# FORTIFY_SOURCE is already set by the packages
CXXFLAGS=
CPPFLAGS=
CC=clang
CXX=clang++

[options]
qt:with_dbus=True