include(${_MZ_CONAN_DIR}/profile.unix.conan)

[settings]
os=Macos
os.version=${CMAKE_OSX_DEPLOYMENT_TARGET}
os_build=Macos
arch=x86_64
arch_build=x86_64
compiler=apple-clang
compiler.version=13.1
compiler.libcxx=libc++
build_type=Release

[env]
CONAN_CMAKE_DISABLE_CHECK_COMPILER=ON

[options]
*:shared=False
*:static=True
libgit2:with_iconv=True
