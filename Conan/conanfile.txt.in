[requires]
${MZ_CONAN_REQUIRES_ITEMS}

[options]
${MZ_CONAN_REQUIRES_OPTS}

[tool_requires]
${MZ_CONAN_TOOL_REQUIRES_ITEMS}

[generators]
cmake

[imports]
bin, *.dll -> ${EXECUTABLE_OUTPUT_PATH}
lib, *.dylib* -> ${EXECUTABLE_OUTPUT_PATH}
., license* -> ${EXECUTABLE_OUTPUT_PATH}/3rdparty @ folder=True, ignore_case=True
