${_MZ_CONAN_PROFILE_INCLUDES}
include(${_MZ_CONAN_DIR}/profile.unix.conan)

[settings]
os=Linux
os_build=Linux
arch=x86_64
arch_build=x86_64
compiler=gcc
compiler.version=5
compiler.libcxx=libstdc++11
build_type=Release

[env]
# FORTIFY_SOURCE is already set by the packages
CXXFLAGS=
CPPFLAGS=

[options]
*:shared=False
*:static=True
